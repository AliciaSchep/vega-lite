{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {"url": "data/cars.json"},
  "layer": [
    {
      "mark": "rule",
      "encoding": {
        "y": {"field": "Origin", "type": "nominal"},
        "x": {
          "field": "Horsepower",
          "aggregate": "min",
          "type": "quantitative"
        },
        "x2": {"field": "Horsepower", "aggregate": "max"}
      }
    },
    {
      "mark": {"type": "point", "opacity": 0, "filled": true},
      "encoding": {
        "y": {"field": "Origin", "type": "nominal"},
        "x": {
          "field": "Horsepower",
          "aggregate": "min",
          "type": "quantitative"
        },
        "x2": {"field": "Horsepower", "aggregate": "max"},
        "label": {
          "field": "Origin",
          "type": "nominal",
          "avoid": {"ancestor": 1}
        }
      }
    }
  ]
}